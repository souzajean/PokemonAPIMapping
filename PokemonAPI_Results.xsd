          </xs:complexType>
        </xs:element>
        <xs:element name="versions">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="generation-ix">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="scarlet-violet">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element type="xs:string" name="front_default"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
